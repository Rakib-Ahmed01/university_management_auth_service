{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-undefined": "warn",
    "no-unreachable": "warn",
    "no-unused-expressions": "warn",
    "no-empty-function": "warn"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  }
}
